{
  "name": "dolan-client-volantis",
  "version": "0.0.0",
  "packageManager": "pnpm@7.13.5",
  "main": "nuxt.config.ts",
  "files": [
    "src",
    "nuxt.config.ts"
  ],
  "scripts": {
    "build:nuxt": "nuxi build",
    "build:schema": "tsx scripts/build-schema.ts",
    "build": "nr build:nuxt && nr build:schema",
    "dev": "nuxi dev",
    "typecheck": "nuxi typecheck",
    "start": "node .output/server/index.mjs",
    "nuxi": "nuxi",
    "lint:js": "eslint .",
    "lint:js:fix": "nr lint:js --fix",
    "lint:css": "stylelint src/**/*.{html,vue,css,scss}",
    "lint:css:fix": "nr lint:css --fix",
    "lint": "nr lint:js && nr lint:css",
    "lint:fix": "nr lint:js:fix && nr lint:css:fix",
    "postinstall": "nuxi prepare"
  },
  "devDependencies": {
    "@antfu/ni": "^0.18.5",
    "@dolan-x/markdown-renderer": "^0.0.38",
    "@dolan-x/shared": "^0.0.38",
    "@iconify-json/carbon": "^1.1.9",
    "@iconify-json/zondicons": "^1.1.2",
    "@leafac/rehype-shiki": "^2.2.1",
    "@nuxtjs/color-mode": "^3.1.8",
    "@nuxtjs/google-fonts": "3.0.0-1",
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge@8.0.0-beta.4-27805200.6d1344c",
    "@pinia/nuxt": "^0.4.3",
    "@so1ve/eslint-config": "^0.31.0",
    "@so1ve/stylelint-config": "^0.3.0",
    "@vueuse/nuxt": "^9.5.0",
    "eslint": "^8.27.0",
    "nuxt": "3.0.0-rc.13",
    "pinia": "^2.0.23",
    "sass": "^1.56.1",
    "shiki-es": "^0.1.2",
    "stylelint": "^14.14.1",
    "ts-json-schema-generator": "^1.1.2",
    "tsx": "^3.12.1",
    "typescript": "^4.8.4",
    "unplugin-icons": "^0.14.13",
    "vite-plugin-toml": "^0.4.0"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "*"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "unplugin",
        "vite",
        "vue",
        "webpack",
        "postcss",
        "rollup",
        "unified",
        "shiki"
      ]
    },
    "overrides": {
      "@leafac/html": "npm:@so1ve/leafac-html"
    }
  }
}
